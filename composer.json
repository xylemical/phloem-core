{
  "name": "xylemical/phloem",
  "description": "Phloem is an extensible framework for executing programmatic style behaviour using configuration..",
  "keywords": [
    "expression-engine",
    "build-framework"
  ],
  "author": {
    "name": "Xylemical"
  },
  "license": "MIT",
  "require": {
    "xylemical/php-expressions": "^1.0",
    "psr/container": "^1.0",
    "pimple/pimple": "^3.2",
    "symfony/yaml": "^4.0",
    "ulrichsg/getopt-php": "^3.1",
    "psr/log": "^1.0",
    "monolog/monolog": "^1.23",
    "bramus/monolog-colored-line-formatter": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Phloem\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phloem\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "bin": [
    "phloem"
  ]
}
